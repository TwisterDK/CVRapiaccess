import apiHeliosCore from '../api/api-helios-core.config';
import { CVRcompany } from './CVRjsonData.type';

export const getSGcompanies = async (): Promise<CVRcompany[]> => {
  const response = await apiHeliosCore.get({ api: 'universal', controller: 'LINK.tblCompanies' });

  // TODO return data-.
  return response.data.value;
};

export default getSGcompanies;
